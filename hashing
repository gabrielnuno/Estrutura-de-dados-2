import java.util.Scanner;
public class MenuHas {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int op;
        System.out.println("Informe o que deseja fazer? ");
        System.out.println("1- Inicializar Hasing");
        System.out.println("2- Sair programa");
        op = in.nextInt();
        
        do{
           switch(op){
               case 1:
                      break;
               case 0: System.out.println("Saindo do programa \n");
                      break;
               default: System.out.println("Opção inválida");
                       break;
           }
        }while(op!=0);
    }
}

package menuhas;

public class GETeSET {
    private final int Has;

    public GETeSET(int Has) {
        this.Has = Has;
    }

    public int getHas() {
        return Has;
    }

    @Override
    public String toString() {
        return "GETeSET{" + "Has=" + Has + '}';
    }

